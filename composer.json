{
	"name" : "internetrix/silverstripe-googletagmanager",
	"description" : "This module contains a couple of handy FormFields for managing sitewide scripts, such as analytics javascript.",
	"type" : "silverstripe-vendormodule",
	"homepage": "https://gitlab.internetrix.net/silverstripe-modules/irxgoogletagmanager",
	"keywords" : [
		"silverstripe",
		"google tag management",
		"GTM",
		"google analytics",
		"analytics  script"
	],
	"license": "BSD-3-Clause",
	"authors" : [{
			"name" : "Guy Watson",
			"email" : "guy.watson@internetrix.com.au"
		}
	],
	"require" : {
		"silverstripe/framework" : "^4"
	},
	"extra" : {
		"installer-name" : "googletagmanager",
		"expose" : [
			"css"
		]
	},
	"replace": {
        "silverstripe-modules/irxgoogletagmanager": "self.version"
    },
	"autoload": {
		"psr-4": {
			"Internetrix\\GoogleTagManager\\": "src/"
		}
	},
	"minimum-stability" : "dev"
}